{
  "name": "lodash-omitdeep",
  "version": "1.1.3",
  "description": "Lodash omitDeep/omitByDeep object key/value recursively",
  "author": {
    "name": "SIBERIA CAN CODE ðŸ§Š",
    "url": "https://github.com/siberiacancode"
  },
  "contributors": [
    {
      "name": "Dmitry Babin",
      "url": "https://github.com/debabin"
    }
  ],
  "license": "MIT",
  "main": "dist/cjs/index.js",
  "module": "dist/esm/index.js",
  "types": "dist/index.d.ts",
  "keywords": [
    "lodash",
    "omit",
    "omitDeep",
    "omitBy",
    "omitByDeep",
    "key",
    "keys",
    "delete",
    "remove",
    "object",
    "deep"
  ],
  "homepage": "https://github.com/siberiacancode/lodash-omitdeep",
  "repository": {
    "type": "git",
    "url": "https://github.com/siberiacancode/lodash-omitdeep"
  },
  "bugs": {
    "url": "https://github.com/siberiacancode/lodash-omitdeep/issues"
  },
  "files": [
    "dist/**/*"
  ],
  "scripts": {
    "build": "shx rm -rf dist && rollup -c --bundleConfigAsCjs",
    "prepublishOnly": "npm test && npm run lint",
    "preversion": "npm run lint",
    "version": "npm run format && git add -A src",
    "prepare": "husky install && npm run build",
    "postversion": "git push && git push --tags",
    "lint": "eslint . --ext ts --ext tsx --no-error-on-unmatched-pattern",
    "format": "prettier --write src/**/*.{ts,tsx}",
    "type": "tsc --noEmit",
    "pretty": "yarn type && yarn format && yarn lint --fix",
    "test": "jest --config jestconfig.json"
  },
  "lint-staged": {
    "*.js": "yarn format",
    "*.ts": "yarn pretty"
  },
  "peerDependencies": {
    "lodash.isnil": "*",
    "lodash.isplainobject": "*",
    "lodash.omit": "*",
    "lodash.omitby": "*"
  },
  "dependencies": {
    "lodash.isnil": "^4.0.0",
    "lodash.isplainobject": "^4.0.6",
    "lodash.omit": "^4.5.0",
    "lodash.omitby": "^4.6.0"
  },
  "devDependencies": {
    "@rollup/plugin-commonjs": "^25.0.0",
    "@rollup/plugin-node-resolve": "^15.1.0",
    "@rollup/plugin-terser": "^0.4.3",
    "@rollup/plugin-typescript": "^11.1.1",
    "@types/jest": "^29.5.1",
    "@types/lodash.isnil": "^4.0.7",
    "@types/lodash.isplainobject": "^4.0.7",
    "@types/lodash.omit": "^4.5.7",
    "@types/lodash.omitby": "^4.6.7",
    "@typescript-eslint/eslint-plugin": "^5.59.9",
    "@typescript-eslint/parser": "^5.59.9",
    "eslint": "^8.42.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-airbnb-typescript": "^17.0.0",
    "eslint-config-prettier": "^8.7.0",
    "eslint-import-resolver-typescript": "^3.5.5",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-prettier": "^4.2.1",
    "husky": "^8.0.1",
    "jest": "^29.5.0",
    "lint-staged": "^13.2.2",
    "prettier": "^2.8.8",
    "rollup": "^3.24.0",
    "rollup-plugin-dts": "^5.3.0",
    "shx": "^0.3.4",
    "ts-jest": "^29.1.0",
    "typescript": "^5.1.3"
  }
}
