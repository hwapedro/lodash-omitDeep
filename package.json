    "@types/lodash.omit": "^4.5.7",
    "@types/lodash.omitby": "^4.6.7",
    "husky": "^8.0.1",
    "lint-staged": "^14.0.1",
    "rollup": "^3.29.2",
    "rollup-plugin-dts": "^6.0.2",
    "shx": "^0.3.4",
